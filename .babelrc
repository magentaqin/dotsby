{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "@babel/plugin-proposal-class-properties",
    ["import-static-files", {"baseDir": "/static/media", "hash": true, "outDir": "../../build" }],
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "@src": "./src",
        "@docs": "./docs",
        "@schema": "./dotsby-schema"
      }
    }]
  ]
}