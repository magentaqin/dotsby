{
  "name": "dotsby",
  "version": "0.1.0",
  "description": "Dotsby Api Doc Generator",
  "main": "index.js",
  "bin": {
    "dotsby": "./bin/dotsby.js"
  },
  "scripts": {
    "build": "react-app-rewired build",
    "start": "react-app-rewired start",
    "bootstrap": "REACT_APP_PORT=8080 babel-node ./src/lib/bootstrap.js",
    "hot-preview": "nodemon --watch . --exec 'yarn build-app && yarn runbuild' --ignore build/",
    "build-docs": "REACT_APP_PORT=8080 babel-node ./src/lib/index.js",
    "lint": "eslint ./src/App --fix",
    "convert": "schema convert ./schemas/src -o ./schemas/dist",
    "pre-publish": "babel ./bin/run.js --out-file ./bin/run.es5.js"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "axios": "^0.19.0",
    "babel-plugin-file-loader": "^2.0.0",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "commander": "^4.1.1",
    "customize-cra": "^0.9.1",
    "datatype-expansion": "^0.3.8",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0",
    "history": "^4.10.1",
    "http-proxy-middleware": "^1.0.1",
    "jsonschema": "^1.2.5",
    "nodemon": "^1.19.1",
    "qs": "^6.9.1",
    "raml-1-parser": "1.1.9",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.0",
    "react-router-dom": "^5.1.0",
    "react-scripts": "^3.1.1",
    "redux": "^4.0.4",
    "remark": "^11.0.2",
    "remark-html": "^10.0.0",
    "remark-preset-lint-recommended": "^3.0.3",
    "styled-components": "^4.3.2",
    "styled-reset": "^4.0.1"
  },
  "author": "Mu Qin",
  "license": "MIT",
  "proxy": "http://localhost:4000"
}
